{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "BackendsResponse",
  "type": "object",
  "additionalProperties": false,
  "required": [
    "correlation_id",
    "backends"
  ],
  "properties": {
    "correlation_id": {
      "type": "string"
    },
    "trace_id": {
      "type": "string"
    },
    "backends": {
      "type": "array",
      "items": {
        "type": "object",
        "additionalProperties": false,
        "required": [
          "name",
          "kind",
          "enabled"
        ],
        "properties": {
          "name": {
            "type": "string"
          },
          "kind": {
            "type": "string",
            "enum": [
              "lexical",
              "vector",
              "graph"
            ]
          },
          "enabled": {
            "type": "boolean"
          },
          "details": {
            "type": "object"
          }
        }
      }
    }
  }
}